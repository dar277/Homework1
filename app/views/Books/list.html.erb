<h1>Reading list<h1>


<% all_books = Book.read %>
<table border=1>
  <thead>
  <tr>
    <th> Title </th>
    <th> Author </th>
  </tr>
  </thead>
<tbody>
<% all_books.each do |book| %>
  <tr>
    <td> <%= book["title"] %> </td>
    <td> <%= book["author"] %> </td>
  </tr>
<% end %>
</tbody>
</table>
